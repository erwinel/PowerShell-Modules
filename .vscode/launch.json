    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run IOUtility Pester Tests",
            "type": "PowerShell",
            "request": "launch",
            "script": "Invoke-Pester",
            "cwd": "${workspaceFolder}/dist/Erwine.Leonard.T.IOUtility",
            "createTemporaryIntegratedConsole": true,
            "attachDotnetDebugger": false,
            "preLaunchTask": "Publish IOUtility"
        },
        {
            "name": "Run IOUtility Pester Tests (no deploy)",
            "type": "PowerShell",
            "request": "launch",
            "script": "Invoke-Pester",
            "cwd": "${workspaceFolder}/dist/Erwine.Leonard.T.IOUtility",
            "createTemporaryIntegratedConsole": true,
            "attachDotnetDebugger": true
        },
        {
            "type": "PowerShell",
            "request": "launch",
            "name": "Deploy IOUtility Local",
            "script": "${workspaceRoot}/Deploy.ps1",
            "args": [
                "-ModuleManifestPath", "${workspaceFolder}/src/IOUtility/Erwine.Leonard.T.IOUtility.psd1",
                "-DeployPSModulePath"
            ],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": "Publish IOUtility"
        },
        {
            "type": "PowerShell",
            "request": "launch",
            "name": "Test SwPackage",
            "script": "${workspaceRoot}/Deploy.ps1",
            "args": [
                "-ModuleManifestPath", "${workspaceFolder}/src/SwPackage/Erwine.Leonard.T.SwPackage.psd1",
                "-DeploymentRoot", "${workspaceFolder}/dist",
                "-PublishLocalRepo -Test"
            ],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": "Publish SwPackage"
        },
        {
            "type": "PowerShell",
            "request": "launch",
            "name": "Test SwPackage (no publish)",
            "script": "${workspaceRoot}/Deploy.ps1",
            "args": [
                "-ModuleManifestPath", "${workspaceFolder}/src/SwPackage/Erwine.Leonard.T.SwPackage.psd1",
                "-DeploymentRoot", "${workspaceFolder}/dist",
                "--Test"
            ],
            "cwd": "${workspaceRoot}"
        },
        {
            "type": "PowerShell",
            "request": "launch",
            "name": "Deploy SwPackage Local",
            "script": "${workspaceRoot}/Deploy.ps1",
            "args": [
                "-ModuleManifestPath", "${workspaceFolder}/src/SwPackage/Erwine.Leonard.T.SwPackage.psd1",
                "-DeployPSModulePath"
            ],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": "Publish SwPackage"
        },
        {
            "name": "Run XmlUtility Pester Tests",
            "type": "PowerShell",
            "request": "launch",
            "script": "Invoke-Pester",
            "cwd": "${workspaceFolder}/dist/Erwine.Leonard.T.XmlUtility",
            "createTemporaryIntegratedConsole": true,
            "attachDotnetDebugger": false,
            "preLaunchTask": "Publish XmlUtility"
        },
        {
            "name": "Run XmlUtility Pester Tests (no deploy)",
            "type": "PowerShell",
            "request": "launch",
            "script": "Invoke-Pester",
            "cwd": "${workspaceFolder}/dist/Erwine.Leonard.T.XmlUtility",
            "createTemporaryIntegratedConsole": true,
            "attachDotnetDebugger": true
        },
        {
            "name": "Run LteDev Pester Tests",
            "type": "PowerShell",
            "request": "launch",
            "script": "Invoke-Pester",
            "cwd": "${workspaceFolder}/dist/Erwine.Leonard.T.LteDev",
            "createTemporaryIntegratedConsole": true,
            "attachDotnetDebugger": false,
            "preLaunchTask": "Publish LteDev"
        },
        {
            "name": "Run LteDev Pester Tests (no deploy)",
            "type": "PowerShell",
            "request": "launch",
            "script": "Invoke-Pester",
            "cwd": "${workspaceFolder}/dist/Erwine.Leonard.T.LteDev",
            "createTemporaryIntegratedConsole": true,
            "attachDotnetDebugger": true
        },
        {
            "type": "PowerShell",
            "request": "launch",
            "name": "PowerShell Launch Current File",
            "script": "${file}",
            "args": [],
            "cwd": "${file}"
        },
        {
            "type": "PowerShell",
            "request": "launch",
            "name": "PowerShell Launch Current File in Temporary Console",
            "script": "${file}",
            "args": [],
            "cwd": "${file}",
            "createTemporaryIntegratedConsole": true
        },
        {
            "type": "PowerShell",
            "request": "launch",
            "name": "PowerShell Launch Current File w/Args Prompt",
            "script": "${file}",
            "args": [
                "${command:SpecifyScriptArgs}"
            ],
            "cwd": "${file}"
        },
        {
            "name": "PowerShell Attach to Host Process",
            "type": "PowerShell",
            "request": "attach",
            "runspaceId": 1
        },
        {
            "type": "PowerShell",
            "request": "launch",
            "name": "PowerShell Interactive Session",
            "cwd": "${workspaceRoot}"
        }
    ]
}