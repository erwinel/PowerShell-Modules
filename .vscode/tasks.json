{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build All Release",
            "identifier": "buildAllRelease",
            "type": "shell",
            "command": "${workspaceFolder}/build.bat",
            "args": [
                "-Configuration",
                "Release"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "Re-Build Release",
            "identifier": "rebuildAllRelease",
            "type": "shell",
            "command": "${workspaceFolder}/build.bat",
            "args": [
                "-Targets",
                "Rebuild",
                "-Configuration",
                "Release"
            ],
            "group": "build",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "Build All Debug",
            "identifier": "buildAllDebug",
            "type": "shell",
            "command": "${workspaceFolder}/build.bat",
            "args": [
                "-Configuration",
                "Debug"
            ],
            "group": "build",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "Re-Build All Debug",
            "identifier": "rebuildAllDebug",
            "type": "shell",
            "command": "${workspaceFolder}/build.bat",
            "args": [
                "-Targets",
                "Rebuild",
                "-Configuration",
                "Debug"
            ],
            "group": "build",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "Re-Build LTEDev Debug",
            "identifier": "rebuildLTEDevDebug",
            "type": "shell",
            "command": "${workspaceFolder}/build.bat",
            "args": [
                "-Targets",
                "Rebuild",
                "-Configuration",
                "Debug",
                "-Project",
                "LteDev"
            ],
            "group": "build",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": "$msCompile"
        }
    ]
}